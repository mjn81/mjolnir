{
  "name": "mjolnir",
  "version": "0.0.1",
  "description": "this is mjolnir file management api",
  "main": "src/app.ts",
  "private": true,
  "repository": "https://github.com/mjn81/mjolnir",
  "author": "mjn<mjjnn81@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/app.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/app.js",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "migrate:dev": "dotenv -e .env.development -- yarn prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- yarn prisma migrate deploy",
    "studio:prod": "dotenv -e .connect.env -- yarn prisma studio",
    "lint": "eslint src/**/*.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "4.2.0",
    "argon2": "^0.28.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-joi-validation": "^5.0.1",
    "helmet": "^5.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "rotating-file-stream": "^3.0.4",
    "swagger-ui-express": "^4.5.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
