{
  "name": "mjolnir",
  "version": "0.0.1",
  "description": "this is mjolnir file management & cloud api",
  "main": "app.js",
  "private": true,
  "repository": "https://github.com/mjn81/mjolnir",
  "author": "mjn<mjjnn81@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/app.ts",
    "debug": "NODE_ENV=development nodemon --inspect src/app.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/app.js",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "migrate:dev": "dotenv -e .env.development -- yarn prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- yarn prisma migrate deploy",
    "studio:prod": "dotenv -e .env.production -- yarn prisma studio",
    "studio:dev": "dotenv -e .env.development -- yarn prisma studio",
    "generate": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.178.0",
    "@prisma/client": "4.2.0",
    "argon2": "^0.28.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-joi-validation": "^5.0.1",
    "helmet": "^5.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "3.3.4",
    "rotating-file-stream": "^3.0.4",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "^16.16.0"
  }
}
